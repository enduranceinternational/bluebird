{
  "name": "@eigi/bluebird-ui-com",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "run-p lerna:start docz:start",
    "build": "lerna run --stream build && docz build",
    "test": "lerna run --stream test",
