    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@eigi/prettier-config": "^0.0.1",
    "@svgr/cli": "^4.3.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "fs-extra": "^8.1.0",
    "jest": "^23.4.2",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "watch": "^1.0.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "scripts": {
    "build": "yarn run cleanup && yarn svgr && yarn generateIndex && webpack && yarn svgr:dist",
    "test": "jest",
    "generateIndex": "node scripts/generate-index.js",
